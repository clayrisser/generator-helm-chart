<% if (_.find(databases, { name: 'mongodb'})) { %>

# MongoDB Config
  - variable: config.mongodb.integration
    default: ''
    description: ''
    type: string
    required: false
    label: integration
    group: 'MongoDB Config'
  - variable: config.mongodb.host
    default: ''
    description: ''
    type: string
    required: false
    label: host
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'
  - variable: config.mongodb.database
    default: <%- _.snakeCase(name) %>
    description: ''
    type: string
    required: true
    label: database
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'
  - variable: config.mongodb.password
    default: P@assw0rd
    description: ''
    type: password
    required: false
    label: password
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'
  - variable: config.mongodb.port
    default: 27017
    description: ''
    type: int
    min: 0
    max: 65535
    required: true
    label: port
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'
  - variable: config.mongodb.url
    default: ''
    description: ''
    type: string
    required: false
    label: url
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'
  - variable: config.mongodb.username
    default: ''
    description: ''
    type: string
    required: false
    label: username
    show_if: config.mongodb.integration=
    group: 'MongoDB Config'<% } %>