<% if (_.find(databases, { name: 'elasticsearch'})) { %>

# Elasticsearch Config
  - variable: config.elasticsearch.integration
    default: ''
    description: ''
    type: string
    required: false
    label: integration
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.host
    default: ''
    description: ''
    type: string
    required: false
    label: host
    show_if: config.elasticsearch.integration=
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.database
    default: <%- _.snakeCase(name) %>
    description: ''
    type: string
    required: true
    label: database
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.password
    default: changeme
    description: ''
    type: password
    required: true
    label: password
    show_if: config.elasticsearch.integration=
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.port
    default: 9200
    description: ''
    type: int
    min: 0
    max: 65535
    required: true
    label: port
    show_if: config.elasticsearch.integration=
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.url
    default: ''
    description: ''
    type: string
    required: false
    label: url
    show_if: config.elasticsearch.integration=
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.username
    default: elastic
    description: ''
    type: string
    required: true
    label: username
    show_if: config.elasticsearch.integration=
    group: 'Elasticsearch Config'<% } %>