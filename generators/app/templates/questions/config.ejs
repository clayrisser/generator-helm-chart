

# Config
  - variable: config.clusterProvider
    default: infrastructure
    description: ''
    type: enum
    options:
      - infrastructure
      - eks
      - aks
      - gke
    required: true
    label: 'cluster provider'
    group: Config
  - variable: config.imagePullPolicy
    default: Always
    description: ''
    type: enum
    options:
      - Always
      - IfNotPresent
    required: true
    label: 'pull policy'
    group: Config
  - variable: config.updateStrategy
    default: Recreate
    description: ''
    type: enum
    options:
      - Recreate
      - RollingUpdate
      - OnDelete
    required: true
    label: 'update strategy'
    group: Config<% for (var i = 0; i < config.length; i++) { configItem = config[i]; %>
  - variable: config.<%- _.camelCase(configItem.key) %>
    default: <%- quote(configItem.defaultValue) %>
    description: ''
    type: <%- configItem.type %>
    required: <%- configItem.required %>
    label: <%- quote(configItem.label) %>
    group: Config<% } %>