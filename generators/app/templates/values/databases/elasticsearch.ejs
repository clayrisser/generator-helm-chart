<% if (_.find(databases, { name: 'elasticsearch'})) { %>
  elasticsearch:
    commonConfig: ''
    dataConfig: ''
    database: <%- name %>
    host: ''
    internal: true
    kibana: false
    masterConfig: ''
    password: elastic
    pause: true
    port: 9200
    prometheus: false
    replicas: 1
    terminationPolicy: Delete
    url: ''
    username: elastic
    version: 6.4.0
    resources:
      requests:
        cpu: 250m
        memory: 512Mi
      limits:
        cpu: 500m
        memory: 1Gi
    versionMap:
      5.6-v1: 5.6
      5.6.4-v1: 5.6.4
      6.2-v1: 6.2
      6.2.4-v1: 6.2.4
      6.3-v1: 6.3
      6.3.0-v1: 6.3.0
      6.4: 6.4
      6.4.0: 6.4.0
    topology:
      enabled: false
      client:
        replicas: 1
        resources:
          requests:
            cpu: 100m
            memory: 64Mi
          limits:
            cpu: 500m
            memory: 128Mi
      data:
        replicas: 1
        resources:
          requests:
            cpu: 100m
            memory: 64Mi
          limits:
            cpu: 500m
            memory: 128Mi
      master:
        replicas: 1
        resources:
          requests:
            cpu: 100m
            memory: 64Mi
          limits:
            cpu: 500m
            memory: 128Mi<% } %>