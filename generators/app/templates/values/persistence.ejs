  enabled: false
  existingClaim: ''
  size: 1Gi
  storageClass: ''
  velero:
    enabled: false
  stash:
    bucket: ''
    container: ''
    enabled: false
    endpoint: s3.amazonaws.com
    keepLast: 30
    paused: false
    prefix: stash
    schedule: '0 0 * * *'
    secret: stash-config-stash-config
    type: s3
    restore:
      snapshot:
        data: ''<% for (var i = 0; i < databases.length; i++) { database = databases[i]; if (database.name !== 'redis') { %>
        <%- _.camelCase(database.name) %>: ''<% }} %>